{
  "type": "object",
  "properties": {
    "isMyView": {
      "type": "identifier",
      "selector": ".subtle[href*='view_all_set.php']",
      "control": "exists"
    },
    "viewIssuesLink": {
      "type": "action",
      "selector": "a",
      "control": "action.link",
      "selectContains": "View Issues"
    }
  },
  "connect": [
    {
      "match": [
        "isMyView"
      ],
      "action": "viewIssuesLink"
    }
  ]
}
